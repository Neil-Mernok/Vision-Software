                    </Grid.RowDefinitions>
                    <avalonEdit:TextEditor x:Name="TextBox_comlog" Background="#EEE"  Grid.Row="1" FontFamily="Consolas" VerticalAlignment="Stretch" Margin="5,0"/>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <!--<ColumnDefinition x:Name="Col_Log" Width="22*"/>-->
